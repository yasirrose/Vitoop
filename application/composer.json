{
    "name": "vitoop",
    "license": "MIT",
    "type": "project",
    "description": "Vitoop.org",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.2.*",
        "symfony/console": "3.2.*",
        "symfony/intl": "3.2.*",
        "symfony/assetic-bundle": "2.8.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.10.*",
        "doctrine/orm": "2.5.4",
        "doctrine/cache": "1.6.0",
        "doctrine/doctrine-bundle": "1.6.*",
        "twig/twig":"1.29.0",
        "twig/extensions": "1.3.*",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.9",
        "incenteev/composer-parameter-handler": "2.1.2",
        "kriswallsmith/buzz": "v0.15",
        "guzzlehttp/guzzle": "6.2.0",
        "doctrine/migrations": "v1.4.1",
        "doctrine/doctrine-migrations-bundle": "1.1.1",
        "jms/di-extra-bundle": "1.7.0",
        "jms/serializer-bundle": "1.1.0",
        "knplabs/knp-snappy-bundle": "^1.4",
        "beberlei/DoctrineExtensions": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1.2",
        "symfony/phpunit-bridge": "^3.2",
        "phpunit/phpunit": "^6.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "platform": {
            "php": "7.0.2"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
